<form method="POST" enctype="mulitpart/form-data" class="signUpForm">
	<label>First Name</label><input type="" name="name" />
	<label>Last Name</label><input type="" name="last" />
	<label>Username</label><input type="" name="username" />
	<label>Password</label><input type="" name="password" />
	<label>Photo</label><input type="file" name="photo" />
	<label>Occupancy</label><input type="" name="occupancy" />
	<label>Relationship</label><select name="relationship">
		<option value="1">Single</option>
		<option value="2">Dating</option>
		<option value="3">Married</option>
		<option value="4">Divorced</option> 
		<option value="5">Widowed</option>
		<option value="6">It's Complicated</option>
	</select>
	<input type="submit" />
</form>